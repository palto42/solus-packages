From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: Reilly Brogan <reilly@reillybrogan.com>
Date: Fri, 4 Oct 2024 12:05:24 -0500
Subject: [PATCH] qt,qt5: Install icon to non-legacy location

---
 qt/icons/Makefile.am  | 2 +-
 qt5/icons/Makefile.am | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/qt/icons/Makefile.am b/qt/icons/Makefile.am
index c9102e6..8b05410 100644
--- a/qt/icons/Makefile.am
+++ b/qt/icons/Makefile.am
@@ -29,4 +29,4 @@ EXTRA_DIST = data-error.svg \
          visibility_dark.svg
 
 icons_DATA = pinentry.png
-iconsdir = $(datadir)/pixmaps
+iconsdir = $(datadir)/icons/hicolor/48x48/apps
diff --git a/qt5/icons/Makefile.am b/qt5/icons/Makefile.am
index 99c5f71..fcab1df 100644
--- a/qt5/icons/Makefile.am
+++ b/qt5/icons/Makefile.am
@@ -26,4 +26,4 @@ EXTRA_DIST = data-error.svg \
              visibility.svg
 
 icons_DATA = pinentry.png
-iconsdir = $(datadir)/pixmaps
+iconsdir = $(datadir)/icons/hicolor/48x48/apps
